@model Dictionary<string,string>


<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">

    <input type="text" name="name" />
    <input type="text" name="val" />
    <button type="submit">Kaydet</button>
</form>
@foreach (var item in Model)
{
    <h2>@item.Key -> @item.Value  <a type="submit" asp-route-name="@item.Key" asp-action="DeleteItem" asp-controller="HashType">Sil</a> </h2>
}